{
  "name": "baseline-environment-on-kubernetes-cdk8s-typescript",
  "version": "1.0.0",
  "main": "main.js",
  "types": "main.ts",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "import": "cdk8s import",
    "synth": "cdk8s synth --output=cdk8s.out",
    "compile": "tsc --build",
    "watch": "tsc --build -w",
    "test": "jest",
    "build": "pnpm run compile && pnpm run test --updateSnapshot && pnpm run synth",
    "upgrade": "pnpm i cdk8s@latest cdk8s-cli@latest",
    "upgrade:next": "pnpm i cdk8s@next cdk8s-cli@next"
  },
  "dependencies": {
    "cdk8s": "^2.69.63",
    "cdk8s-plus-28": "^2.5.6",
    "constructs": "^10.4.2"
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "^14.18.63",
    "cdk8s-cli": "^2.200.47",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  }
}
